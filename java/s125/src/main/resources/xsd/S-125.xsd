                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="television">
                <xs:annotation>
                    <xs:documentation>broadcast of television signals.</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="radio">
                <xs:annotation>
                    <xs:documentation>broadcast of radio signals.</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="radar">
