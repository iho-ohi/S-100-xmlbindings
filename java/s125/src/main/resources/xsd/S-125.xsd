    <!--<xs:element name="GenericBeacon" type="GenericBeaconType"/>-->
    <xs:complexType name="GenericBeaconType" abstract="true">
        <xs:annotation>
            <xs:documentation>A fixed artificial navigation mark that can be recognised by its shape, colour, pattern, topmark or light character, or a combination of these. It may carry various additional aids to navigation.</xs:documentation>
        </xs:annotation>
        <xs:complexContent>
            <xs:extension base="StructureObjectType">
                <xs:sequence>
                    <xs:element name="beaconShape" type="beaconShapeType" minOccurs="1" maxOccurs="1">
