    <!-- abstract feature types -->
    <!--<xs:element name="AidsToNavigation" type="AidsToNavigationType"/>-->
    <xs:complexType name="AidsToNavigationType" abstract="true">
        <xs:annotation>
            <xs:documentation>A visual, acoustical, or radio device, external to a ship, designed to assist in determining a safe course or a vessel's position, or to warn of dangers and/or obstructions. Aids to navigation usually include buoys, beacons, fog signals, lights, radio beacons, leading marks, radio position fixing systems and GNSS which are chart-related and assist safe navigation.</xs:documentation>
        </xs:annotation>
        <xs:complexContent>
            <xs:extension base="S100:AbstractFeatureType">
                <xs:sequence>
                    <xs:element name="dateEnd" type="S100_TruncatedDate" minOccurs="0" maxOccurs="1">
                        <xs:annotation>
                            <xs:documentation>The latest date on which an object (e.g., a buoy) will be present.</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="dateStart" type="S100_TruncatedDate" minOccurs="0" maxOccurs="1">
