    </xs:complexType>
    <xs:element name="EmergencyWreckMarkingBuoy" type="EmergencyWreckMarkingBuoyType"/>
    <xs:complexType name="EmergencyWreckMarkingBuoyType">
        <xs:annotation>
            <xs:documentation>An emergency wreck marking buoy is a buoy moored on or above a new wreck, designed to provide a prominent (both visual and radio) and easily identifiable temporary (24-72 hours) first response. (IHO Registry)</xs:documentation>
        </xs:annotation>
        <xs:complexContent>
            <xs:extension base="GenericBuoyType">
                <xs:sequence>
                    <xs:element name="geometry" maxOccurs="unbounded">
