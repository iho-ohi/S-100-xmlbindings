    <xs:element name="Pile" type="PileType"/>
    <xs:complexType name="PileType">
        <xs:annotation>
            <xs:documentation>A long heavy timber or section of steel, wood, concrete, etc., forced into the earth or sea floor to serve as a support, as for a pier, or to resist lateral pressure; or as a free standing pole within a marine environment.</xs:documentation>
        </xs:annotation>
        <xs:complexContent>
            <xs:extension base="StructureObjectType">
                <xs:sequence>
                    <xs:element name="categoryOfPile" type="categoryOfPileType" minOccurs="0" maxOccurs="1">
                        <xs:annotation>
                            <xs:documentation>Classification of pile, driven into the earth as a foundation or support for a structure.</xs:documentation>
                        </xs:annotation>
