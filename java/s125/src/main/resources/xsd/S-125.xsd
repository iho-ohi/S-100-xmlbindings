    <xs:simpleType name="categoryOfPileType">
        <xs:restriction base="xs:string">
            <xs:enumeration value="Stake">
                <xs:annotation>
                    <xs:documentation>An elongated wood or metal pole embedded in the seabed to serve as a marker or support.</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="Post">
                <xs:annotation>
                    <xs:documentation>A vertical piece of timber, metal or concrete forced into the earth or sea bed.</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="Tripodal">
                <xs:annotation>
                    <xs:documentation>A single structure comprising 3 or more piles held together (sections of heavy timber, steel or concrete), and forced into the earth or sea bed.</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="Piling">
                <xs:annotation>
                    <xs:documentation>A number of piles, usually in a straight line, and usually connected or bolted together.</xs:documentation>
