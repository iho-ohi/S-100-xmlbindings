    <xs:element name="RecommendedTrack" type="RecommendedTrackType"/>
    <xs:complexType name="RecommendedTrackType">
        <xs:annotation>
            <xs:documentation>A route which has been specially examined to ensure so far as possible that it is free of dangers and along which ships are advised to navigate.</xs:documentation>
        </xs:annotation>
        <xs:complexContent>
            <xs:extension base="AidsToNavigationType">
                <xs:sequence>
