                    <xs:element name="natureOfConstruction" type="natureOfConstructionType" minOccurs="0" maxOccurs="unbounded">
                        <xs:annotation>
                            <xs:documentation>The buildings primary construction material</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="status" type="statusType" minOccurs="0" maxOccurs="unbounded">
                        <xs:annotation>
                            <xs:documentation>The condition of an object at a given instant in time.</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="verticalLength" type="xs:double" minOccurs="0" maxOccurs="1">
                        <xs:annotation>
                            <xs:documentation>The total vertical length of an object.</xs:documentation>
