<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:S100="http://www.iho.int/s100gml/5.0" xmlns:gml="http://www.opengis.net/gml/3.2" xmlns="http://www.iho.int/S-125/gml/cs0/1.0" targetNamespace="http://www.iho.int/S-125/gml/cs0/1.0" elementFormDefault="qualified" version="1.0.0">
