            <xs:enumeration value="IALA B">
                <xs:annotation>
                    <xs:documentation>navigational aids conform to the International Association of Lighthouse Authorities - IALA B system.</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="no system">
                <xs:annotation>
                    <xs:documentation>navigational aids do not conform to any defined system.</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="other system">
