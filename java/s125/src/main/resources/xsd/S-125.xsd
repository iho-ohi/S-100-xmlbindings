    <xs:element name="LightFloat" type="LightFloatType"/>
    <xs:complexType name="LightFloatType">
        <xs:annotation>
            <xs:documentation>A boat-like structure used instead of a light buoy in waters where strong streams or currents are experienced, or when a greater elevation than that of a light buoy is necessary.</xs:documentation>
        </xs:annotation>
        <xs:complexContent>
            <xs:extension base="StructureObjectType">
                <xs:sequence>
                    <xs:element name="colour" type="colourType" minOccurs="1" maxOccurs="unbounded">
                        <xs:annotation>
                            <xs:documentation>The property possessed by an object of producing different sensations on the eye as a result of the way it reflects or emits light.</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="colourPattern" type="colourPatternType" minOccurs="0" maxOccurs="unbounded">
