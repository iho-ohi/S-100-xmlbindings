            <xs:enumeration value="Timber">
                <xs:annotation>
                    <xs:documentation>Wood prepared for use in building or carpentry.</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="Sawdust/Wood Chips">
                <xs:annotation>
                    <xs:documentation>Powdery fragments of wood made in sawing timber or coarse chips produced for use in manufacturing pressed board.</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="Scrap Metal">
                <xs:annotation>
                    <xs:documentation>Discarded metal suitable for being reprocessed.</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="Liquefied Natural Gas">
                <xs:annotation>
                    <xs:documentation>Natural gas that has been liquefied for ease of transport by cooling the gas to -162 Celsius.</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="Liquefied Petroleum Gas">
                <xs:annotation>
                    <xs:documentation>A compressed gas consisting of flammable light hydrocarbons and derived from petroleum.</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="Wine">
                <xs:annotation>
                    <xs:documentation>The fermented juice of grapes.</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="Cement">
                <xs:annotation>
                    <xs:documentation>A substance made of powdered lime and clay, mixed with water.</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="Grain">
                <xs:annotation>
                    <xs:documentation>A small hard seed, especially that of any cereal plant such as wheat, rice, corn, rye etc.</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="Electricity">
                <xs:annotation>
                    <xs:documentation>Electric charge or current.</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="Ice">
                <xs:annotation>
                    <xs:documentation>The solid form of water.</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="Clay">
