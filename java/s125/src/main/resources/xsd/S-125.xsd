    <xs:simpleType name="lightCharacteristicType">
        <xs:restriction base="xs:string">
            <xs:enumeration value="fixed">
                <xs:annotation>
                    <xs:documentation>a signal light that shows continuously, in any given direction, with constant luminous intensity and colour.</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="flashing">
                <xs:annotation>
                    <xs:documentation>a rhythmic light in which the total duration of light in a period is clearly shorter than the total duration of darkness and all the appearances of light are of equal duration.</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="long-flashing">
                <xs:annotation>
                    <xs:documentation>a flashing light in which a single flash of not less than two seconds duration is regularly repeated.</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="quick-flashing">
                <xs:annotation>
                    <xs:documentation>a light exhibiting without interruption very rapid regular alternations of light and darkness.</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="very quick-flashing">
                <xs:annotation>
                    <xs:documentation>a flashing light in which flashes are repeated at a rate of not less than 80 flashes per minute but less than 160 flashes per minute.</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="ultra quick-flashing">
                <xs:annotation>
                    <xs:documentation>a flashing light in which flashes are repeated at a rate of not less than 160 flashes per minute.</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="isophased">
                <xs:annotation>
                    <xs:documentation>a light with all durations of light and darkness equal.</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="occulting">
                <xs:annotation>
                    <xs:documentation>a rhythmic light in which the total duration of light in a period is clearly longer than the total duration of darkness and all the eclipses are of equal duration.</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="interrupted quick-flashing">
                <xs:annotation>
                    <xs:documentation>a quick light in which the sequence of flashes is interrupted by regularly repeated eclipses of constant and long duration.</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="interrupted very quick-flashing">
                <xs:annotation>
                    <xs:documentation>a light in which the very rapid alterations of light and darkness are interrupted at regular intervals by eclipses of long duration.</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="interrupted ultra quick-flashing">
                <xs:annotation>
                    <xs:documentation>a light in which the ultra quick flashes (160 or more per minute) are interrupted at regular intervals by eclipses of long duration.</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="morse">
                <xs:annotation>
                    <xs:documentation>a rhythmic light in which appearances of light of two clearly different durations are grouped to represent a character or characters in the Morse code.</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="fixed and flash">
                <xs:annotation>
                    <xs:documentation>a rhythmic light in which a fixed light is combined with a flashing light of higher luminous intensity.</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="flash and long-flash">
                <xs:annotation>
                    <xs:documentation/>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="occulting and flash">
                <xs:annotation>
                    <xs:documentation/>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="fixed and long-flash">
                <xs:annotation>
                    <xs:documentation/>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="occulting alternating">
                <xs:annotation>
                    <xs:documentation/>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="long-flash alternating">
                <xs:annotation>
                    <xs:documentation/>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="flash alternating">
                <xs:annotation>
                    <xs:documentation/>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="quick-flash plus long-flash">
                <xs:annotation>
                    <xs:documentation/>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="very quick-flash plus long-flash">
                <xs:annotation>
                    <xs:documentation/>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="ultra quick-flash plus long-flash">
                <xs:annotation>
                    <xs:documentation/>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="alternating">
                <xs:annotation>
                    <xs:documentation>a signal light that shows, in any given direction, two or more colours in a regularly repeated sequence with a regular periodicity.</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="lightVisibilityType">
        <xs:restriction base="xs:string">
            <xs:enumeration value="high intensity">
                <xs:annotation>
                    <xs:documentation>non-marine lights with a higher power than marine lights and visible from well off shore (often 'Aero' lights).</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="low intensity">
                <xs:annotation>
                    <xs:documentation>non-marine lights with lower power than marine lights.</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="faint">
                <xs:annotation>
                    <xs:documentation>a decrease in the apparent intensity of a light which may occur in the case of partial obstructions.</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="intensified">
                <xs:annotation>
                    <xs:documentation>a light in a sector is intensified (i.e. has longer range than other sectors).</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="unintensified">
                <xs:annotation>
                    <xs:documentation>a light in a sector is unintensified (i.e. has shorter range than other sectors).</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="visibility deliberately restricted">
                <xs:annotation>
                    <xs:documentation>a light sector is deliberately reduced in intensity, for example to reduce its effect on a built-up area.</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="obscured">
                <xs:annotation>
                    <xs:documentation>said of the arc of a light sector designated by its limiting bearings in which the light is not visible from seaward.</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="partially obscured">
                <xs:annotation>
                    <xs:documentation>this value specifies that parts of the sector are obscured.</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="marksNavigationalSystemOfType">
        <xs:restriction base="xs:string">
            <xs:enumeration value="IALA A">
                <xs:annotation>
                    <xs:documentation>navigational aids conform to the International Association of Lighthouse Authorities - IALA A system.</xs:documentation>
                </xs:annotation>
