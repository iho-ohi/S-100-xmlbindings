                    <xs:element name="radarConspicuous" type="radarConspicuousType" minOccurs="0" maxOccurs="1">
                        <xs:annotation>
                            <xs:documentation>A feature which returns a strong radar echo.</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="height" type="xs:double" minOccurs="0" maxOccurs="1">
                        <xs:annotation>
                            <xs:documentation>The value of the vertical distance to the highest point of the object, measured from a specified vertical datum.</xs:documentation>
