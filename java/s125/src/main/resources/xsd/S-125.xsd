                    <xs:documentation>A mark indicating a one-way route.</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="Reduced Wake Mark">
                <xs:annotation>
                    <xs:documentation>A mark indicating that vessels must not generate excessive wake.</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="Speed Limit Mark">
                <xs:annotation>
                    <xs:documentation>A mark indicating that a speed limit applies.</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="Stop Mark">
                <xs:annotation>
                    <xs:documentation>A mark indicating the place where the bow of a ship must stop when traffic lights show red.</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="General Warning Mark">
                <xs:annotation>
                    <xs:documentation>A mark indicating that special caution must be exercised in the vicinity of the mark.</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="Sound Ship's Siren Mark">
