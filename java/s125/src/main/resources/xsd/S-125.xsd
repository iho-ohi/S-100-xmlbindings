                        </xs:annotation>
                    </xs:element>
                    <xs:element name="idCode" type="xs:anyURI" minOccurs="1" maxOccurs="1">
                        <xs:annotation>
                            <xs:documentation>Identification code as specified in predefined system. Also called identification number.</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="periodEnd" type="S100_TruncatedDate" minOccurs="0" maxOccurs="1">
                        <xs:annotation>
                            <xs:documentation>Period end</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="periodStart" type="S100_TruncatedDate" minOccurs="0" maxOccurs="1">
                        <xs:annotation>
                            <xs:documentation>Period start</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="pictorialRepresentation" type="xs:string" minOccurs="0" maxOccurs="1">
                        <xs:annotation>
                            <xs:documentation>Indicates whether a pictorial representation of the object is available.</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="scaleMinimum" type="xs:integer" minOccurs="0" maxOccurs="1">
                        <xs:annotation>
                            <xs:documentation>The minimum scale at which the object may be used, e.g., for ECDIS presentation.</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="featureName" type="featureNameType" minOccurs="0" maxOccurs="unbounded">
                        <xs:annotation>
                            <xs:documentation>file reference to text file</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="information" type="informationType" minOccurs="0" maxOccurs="unbounded">
