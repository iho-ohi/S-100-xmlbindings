    <xs:element name="Building" type="BuildingType"/>
    <xs:complexType name="BuildingType">
        <xs:annotation>
            <xs:documentation>A free-standing self-supporting construction that is roofed, usually walled, and is intended for human occupancy (for example: a place of work or recreation) and/or habitation.</xs:documentation>
        </xs:annotation>
        <xs:complexContent>
            <xs:extension base="StructureObjectType">
