                    <xs:element name="natureOfConstruction" type="natureOfConstructionType" minOccurs="0" maxOccurs="unbounded">
                        <xs:annotation>
                            <xs:documentation>The building&#39;s primary construction material.</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="radarConspicuous" type="xs:boolean" minOccurs="0" maxOccurs="1">
                        <xs:annotation>
                            <xs:documentation>A feature which returns a strong radar echo.</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="status" type="statusType" minOccurs="0" maxOccurs="unbounded">
